#include<stdio.h>
void main()
{
  char str[100],b[]="aeiou AEIOU");
  int count,c;
  printf("enter the string);
scanf("%[^\n]",str);
for (int i=0;str[i]!='\0',i++)
{
if(int j=0;b[j]!='\0';j++)

while 
{
if(str[i]==b[j])
count ++;

}
c++
}
printf ("number of vowels %d\n";count);
printf ("number of consonants %d\n";i-count);
}
